/*--------------------------header----------------------------------*/
#widgetHeader, #frameTitle{
background-image: url(:/OneKeyHide/Resources/bg-black-rect.png);
}

#labelTitle {
	color: rgb(255, 255, 255);
}

#labelLogo {
background-image: url(:/OneKeyHide/Resources/logo20x20.png)
}
#labelBigLogo {
background-image: url(:/OneKeyHide/Resources/logo.png)
}

#labelArrow {
background-image: url(:/OneKeyHide/Resources/arrow.png);
background-repeat: repeat-y
}
/*----------------------------菜单栏按钮------------------------------------*/
/*----------------------------菜单栏按钮------------------------------------*/
QPushButton#pushButtonMenu{
	border-style:flat;
	background-image: url(:/OneKeyHide/Resources/menu/menu.png);
	background-repeat:no-repeat;
	background-position:center;
}
QPushButton#pushButtonMenu:hover:pressed{background-color:#3694da;border-style:flat;}
QPushButton#pushButtonMenu:hover:!pressed{background-color:rgb(85, 200, 255);border-style:flat;}

QPushButton#pushButtonMin{
	border-style:flat;
	background-image: url(:/OneKeyHide/Resources/menu/min.png);
	background-repeat:no-repeat;
	background-position:center;
}
QPushButton#pushButtonMin:hover:pressed{background-color:red;border-style:flat;}
QPushButton#pushButtonMin:hover:!pressed{background-color:red;border-style:flat;}

QPushButton#pushButtonMax{
	border-style:flat;
	background-image: url(:/OneKeyHide/Resources/menu/max.png);
	background-repeat:no-repeat;
	background-position:center;
}
QPushButton#pushButtonMax:hover:pressed{background-color:red;border-style:flat;}
QPushButton#pushButtonMax:hover:!pressed{background-color:red;border-style:flat;}

QPushButton#pushButtonClose{
	border-style:flat;
	background-image: url(:/OneKeyHide/Resources/menu/close.png);
	background-repeat:no-repeat;
	background-position:center;
}
QPushButton#pushButtonClose:hover:pressed{background-color:red;border-style:flat;}
QPushButton#pushButtonClose:hover:!pressed{background-color:red;border-style:flat;}

/*----------------------------通用按钮---------------------------------------*/
/*----------------------------通用按钮---------------------------------------*/
#pushButtonSettingsReset,#pushButtonSettingsSave,#pushButtonCancel,#pushButtonOk,#pushButtonTest,#pushButtonRefresh {
	border-style:flat;
	background-image: url(:/OneKeyHide/Resources/button/btn_bg1.png);
}
#pushButtonSettingsReset:hover:pressed,
#pushButtonSettingsSave:hover:pressed,
#pushButtonCancel:hover:pressed,
#pushButtonOk:hover:pressed,
#pushButtonTest:hover:pressed,
#pushButtonRefresh:hover:pressed {
	background-image: url(:/OneKeyHide/Resources/button/btn_bg1.png);
}
#pushButtonSettingsReset:hover:!pressed,
#pushButtonSettingsSave:hover:!pressed,
#pushButtonCancel:hover:!pressed,
#pushButtonOk:hover:!pressed,
#pushButtonTest:hover:!pressed,
#pushButtonRefresh:hover:!pressed {
	background-image: url(:/OneKeyHide/Resources/button/btn_bg2.png);
}


