QLineEdit{
border-radius:2px;
border:1px solid #e1e0e0;
background-color: rgb(255, 255, 255);
}
QLineEdit:hover{
border-radius: 3px;
border-width:2px;
border-color:rgb(50, 180, 250);
}

QComboBox{
border-radius:2px;
border:1px solid gray;
}
QComboBox:hover{
border-radius: 3px;
border-width:2px;
border-color:rgb(50, 180, 250);
}
QComboBox::down-arrow{
image:url(:/OneKeyHide/Resources/OneKeyHide/img/DragDownBig.png);
}
QComboBox::drop-down{
width:16px;
border-left:1px solid gray;
background:transparent;
}
QComboBox::drop-down:hover{
border-left:1px solid lightgray;
background:rgb(220, 240, 250);
}
QListWidget {
border: none;
}

QWidget {
